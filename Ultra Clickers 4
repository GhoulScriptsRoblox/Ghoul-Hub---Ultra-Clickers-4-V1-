-- Scripts:

local function NMJOGX_fake_script() -- AutoClickerOFF.LocalScript 
	local script = Instance.new('LocalScript', AutoClickerOFF)

	AutoClickerOFF.MouseButton1Down:connect(function()
		getgenv().AutoClickerOn = false;

		--[[while wait() do
			if getgenv().AutoClickerOn == true then
				workspace.Events.AddClick:FireServer()
			end
		end--]]
	end)
end
coroutine.wrap(NMJOGX_fake_script)()
local function HOCP_fake_script() -- AutoOpenBestEggOFF.LocalScript 
	local script = Instance.new('LocalScript', AutoOpenBestEggOFF)

	AutoOpenBestEggOFF.MouseButton1Down:connect(function()
		getgenv().AutoOpenBestEggOn = false;

		--[[while wait() do
			if getgenv().AutoOpenBestEggOn == true then
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-5873.92773, -25.9682026, -2401.71484, -0.0709527731, 5.73836552e-08, 0.997479677, -6.11522921e-08, 1, -6.18785307e-08, -0.997479677, -6.5388619e-08, -0.0709527731)
		
		local args = {
			[1] = "GhostEgg",
			[2] = "Single"
		}

		game:GetService("ReplicatedStorage").RemoteEvents.EggOpened:InvokeServer(unpack(args))
			end
		end--]]
	end)
end
coroutine.wrap(HOCP_fake_script)()
local function PBPKIQT_fake_script() -- AutoClickerON.LocalScript 
	local script = Instance.new('LocalScript', AutoClickerON)

	AutoClickerON.MouseButton1Down:connect(function()
		getgenv().AutoClickerOn = true;

		while wait() do
			if getgenv().AutoClickerOn == true then
				workspace.Events.AddClick:FireServer()
			end
		end
	end)
end
coroutine.wrap(PBPKIQT_fake_script)()
local function CONC_fake_script() -- AutoOpenBestEggON.LocalScript 
	local script = Instance.new('LocalScript', AutoOpenBestEggON)

	AutoOpenBestEggON.MouseButton1Down:connect(function()
		getgenv().AutoOpenBestEggOn = true;

		while wait() do
			if getgenv().AutoOpenBestEggOn == true then
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-5873.92773, -25.9682026, -2401.71484, -0.0709527731, 5.73836552e-08, 0.997479677, -6.11522921e-08, 1, -6.18785307e-08, -0.997479677, -6.5388619e-08, -0.0709527731)

				local args = {
					[1] = "GhostEgg",
					[2] = "Single"
				}

				game:GetService("ReplicatedStorage").RemoteEvents.EggOpened:InvokeServer(unpack(args))
			end
		end
	end)
end
coroutine.wrap(CONC_fake_script)()
local function GKIM_fake_script() -- SpawnTP_2.LocalScript 
	local script = Instance.new('LocalScript', SpawnTP_2)

	SpawnTP.MouseButton1Down:connect(function()
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1174.34143, -40.7038803, 578.889709, 0.999416232, 0.000104351646, -0.0341643281, -0.000114671726, 0.99999994, -0.000300112704, 0.0341642946, 0.000303855166, 0.999416173)
	end)
end
